{
  "compilerOptions": {
    "target": "es6",
    "module": "CommonJS",
    "lib": ["ES5", "ES2015", "ES6"],
    "outDir": "./bin",
    "rootDir": "./src",
    "strict": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "types": ["../../core"],
    "typeRoots": ["@types"],
    "experimentalDecorators": true,
  }
}
